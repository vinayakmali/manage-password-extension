{
    "name": "Auto Prefill Password",
    "manifest_version": 2,
    "version": "1.0",
    "description": "This Chrome extension to auto-prefill password.",
    "permissions": [
        "contextMenus",
       "tabs",
      "notifications",
       "identity",
    "identity.email",
      "http://*/*",
      "https://*/*"
    ],
    "background": {
        "scripts": ["js/background.js"]
    },
    "browser_action" : { 
            "default_popup": "popup.html",  

        "default_icon" : "images/icon.png"
    },
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128":"images/icon.png"
    }
}
